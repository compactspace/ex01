package kr.happyjob.study.managepost.model;

// 채용담당장 입사지원자들 관리
public class ApplicantModel {
	public int appId; // 입사지원PK
	public int userIdx; // 회원FK
	public String loginId; // 회원아이디
	public int postIdx; // 공고FK
	public String title; // post제목
	public String endDate; // 채용기간
	public String applyDate; // 지원날짜
	public int viewed; // 열람유무
	public String status; // 지원현황
	public int resIdx; // 이력서ID
	private String	name; // 회원 이름
	private String email; // 회원 이메일
	private String phone; // 회원 전화번호
	private String hirProcess; // 공고 진행절차
	private String resTitle; // 지원자 이력서
	private String schoolName; // 지원자 학교이름
	private String grdStatus; // 지원자 졸업상태
	private String company; // 경력 회사 이름
	
	public String getEndDate() {
		return endDate;
	}
	public void setEndDate(String endDate) {
		this.endDate = endDate;
	}
	public int getPostIdx() {
		return postIdx;
	}
	public void setPostIdx(int postIdx) {
		this.postIdx = postIdx;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	public String getPhone() {
		return phone;
	}
	public void setPhone(String phone) {
		this.phone = phone;
	}
	public String getHirProcess() {
		return hirProcess;
	}
	public void setHirProcess(String hirProcess) {
		this.hirProcess = hirProcess;
	}
	public String getResTitle() {
		return resTitle;
	}
	public void setResTitle(String resTitle) {
		this.resTitle = resTitle;
	}
	public String getSchoolName() {
		return schoolName;
	}
	public void setSchoolName(String schoolName) {
		this.schoolName = schoolName;
	}
	public String getGrdStatus() {
		return grdStatus;
	}
	public void setGrdStatus(String grdStatus) {
		this.grdStatus = grdStatus;
	}
	public String getCompany() {
		return company;
	}
	public void setCompany(String company) {
		this.company = company;
	}
	public int getAppId() {
		return appId;
	}
	public void setAppId(int appId) {
		this.appId = appId;
	}
	public int getUserIdx() {
		return userIdx;
	}
	public void setUserIdx(int userIdx) {
		this.userIdx = userIdx;
	}
	public String getLoginId() {
		return loginId;
	}
	public void setLoginId(String loginId) {
		this.loginId = loginId;
	}
	public String getApplyDate() {
		return applyDate;
	}
	public void setApplyDate(String applyDate) {
		this.applyDate = applyDate;
	}
	public int getViewed() {
		return viewed;
	}
	public void setViewed(int viewed) {
		this.viewed = viewed;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public int getResIdx() {
		return resIdx;
	}
	public void setResIdx(int resIdx) {
		this.resIdx = resIdx;
	}
	

}